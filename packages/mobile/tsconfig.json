{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react-native",
        "lib": [
            "es6",
            "esnext.asynciterable"
        ],
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "noUnusedLocals": true,
        "skipLibCheck": true
    },
    "include": [
        "./**/*"
    ],
    "extends": "expo/tsconfig.base"
}